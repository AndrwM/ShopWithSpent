$green: #8fb61f;
$blue: #4b5a63;
$p: #aaa;
$light: #aaa;
$greyborder: #d4d7d9;
$greenborder: #78991a;
$UnitMed: 'UnitMed','Helvetica Neue',Helvetica,Arial,Sans-Serif;
$UnitReg: 'UnitReg','Helvetica Neue',Helvetica,Arial,Sans-Serif;
$Hel: 'Helvetica Neue',Helvetica,Arial,Sans-Serif;


//----------------- @FF

@font-face {
		font-family: 'UnitMed';
		src: url('fonts/unit-med.eot');
		src: url('fonts/unit-med.eot?#iefix') format('embedded-opentype'),
				 url('fonts/unit-med.woff') format('woff'),
				 url('fonts/unit-med.ttf') format('truetype'),
				 url('fonts/unit-med.svg#unit-med') format('svg');
		font-weight: normal;
		font-style: normal; }

@font-face {
		font-family: 'UnitReg';
		src: url('fonts/unit-reg.eot');
		src: url('fonts/unit-reg.eot?#iefix') format('embedded-opentype'),
				 url('fonts/unit-reg.woff') format('woff'),
				 url('fonts/unit-reg.ttf') format('truetype'),
				 url('fonts/unit-reg.svg#unit-reg') format('svg');
		font-weight: normal;
		font-style: normal; }

//----------------- Mixins

@mixin border-radius($radius) {
	-moz-border-radius: $radius;
	-webkit-border-radius: $radius;
	-ms-border-radius: $radius;
	border-radius: $radius;
}

@mixin box-shadow($value) {
	-webkit-box-shadow: #{$value};
	-moz-box-shadow: #{$value}; 
	box-shadow: #{$value};
}

//----------------- Reset

* { position:relative; margin:0; padding:0; }
img { border:none; }
.hide { display:none; }
.seo { font: 0/0 a; text-shadow: none; color: transparent; }

.clfix:after { content: ""; clear: both; display: block; }
.cl { clear: both; height: 0; font-size: 0; overflow: hidden; }
.left { float:left; }
.right { float:right; }
.container { width:1000px; margin:auto; }

a {
	text-decoration: none;
	&.block { display:block; }
}
ul { list-style:none; }
hr { border:none; }
p {  }

//----------------- Base Styles

* {
	&:focus { outline-color: #c8d49f; }
	&::selection, &::-moz-selection { background: $green; color:#fff; }
}

#main a:not(.btn), #footer a {
	color:$green;
	font-weight:bold;
	&:hover { text-decoration:underline; }
}

input {
	font-size:14px;
	color:$blue;
	background:none;
	border-radius: 3px;
	border:1px solid $greyborder;
	height:27px;
	padding:0 9px;
	@include box-shadow('inset 0px 3px 0px 0px #f7f7f7');
	&::-webkit-input-placeholder {
		color:$greyborder;
	}
	&:-moz-placeholder {  
		color:$greyborder; 
	}
}
a.btn {
	font-size:19px;
	font-weight:bold;
	color:#fff;
	background:$green;
	text-shadow: 0px -1px 1px #295706;
	display:inline-block;
	border-radius:3px;
	padding:9px 20px;
	border:1px solid $greenborder;
	@include box-shadow('0px 0px 0px 3px rgba(0, 0, 0, 0.1), inset 0px 1px 0px 0px rgba(255, 255, 255, .2)');
	&.dropped {
		@include box-shadow('0px 2px 0px 0px rgba(0, 0, 0, 0.1), inset 0px 3px 0px 0px #78991a');
		border-radius:0 0 3px 3px;
		z-index:1;
	}
	&:hover, &:active { background:#87ab1a; }
}
.box {
	background:#fff;
	border-radius:3px;
	border:1px solid $greyborder;	
	@include box-shadow('0px 3px 0px 0px #ededed');
	img.border {
		border-radius:3px;
		border:1px solid #485962;
	}
}

//----------------- Index Sprite Icons

.icnsprite {
	background: url(img/index-sprite.png) no-repeat;
	width: 18px;
    height: 16px;
	display:block;
	&.cart { background-position: -29px -81px; }
	&.chat { background-position: -0px -81px; }
	&.cog { background-position: -87px -54px; }
	&.doc { background-position: -87px -27px; }
	&.globe { background-position: -87px -0px; }
	&.graph { background-position: -58px -54px; }
	&.guide { background-position: -29px -54px; }
	&.iphone { background-position: -0px -54px; }
	&.mail { background-position: -58px -27px; }
	&.pos { background-position: -29px -27px; }
	&.smiley { background-position: -0px -27px; }
	&.tag { background-position: -58px -0px; }
	&.twitter { background-position: -29px -0px; }
	&.userchat { background-position: -0px -0px; }
}

